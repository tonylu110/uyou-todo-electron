    "rolldown": "1.0.0-beta.30",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-dts": "^6.2.1",
    "sass": "^1.82.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "unocss": "^66.3.3",
    "unplugin-element-plus": "^0.10.0",
