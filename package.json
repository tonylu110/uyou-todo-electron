{
  "name": "uyoutodo",
  "version": "2.2.1",
  "private": false,
  "author": {
    "name": "Anthony Lu",
    "email": "taruruchan0@gmail.com"
  },
  "license": "MIT",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron:servewin": "concurrently -k \"pnpm dev\" \"pnpm electron\"",
    "electron:servemac": "concurrently -k \"pnpm dev\" \"pnpm electron:mac\"",
    "electron:servelinux": "concurrently -k \"pnpm dev\" \"pnpm electron:linux\"",
    "electron:buildwin": "vite build && electron-builder",
    "electron:buildwinarm": "vite build && electron-builder --win --arm64",
    "electron:buildmac": "vite build && electron-builder --config.npmRebuild=false -c.extraMetadata.main=electron/mac/mainMac.js",
    "electron": "wait-on tcp:3000 && cross-env NODE_ENV=development electron .",
    "electron:mac": "wait-on tcp:3000 && cross-env NODE_ENV=development electron electron/mac/mainMac.js",
    "electron:linux": "wait-on tcp:3000 && cross-env NODE_ENV=development electron electron/mac/mainMac.js --no-sandbox",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@electron/remote": "^2.0.10",
    "electron-store": "^8.1.0",
    "mica-electron": "^1.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.2",
    "@iconify-json/fluent": "^1.1.39",
    "@iconify-json/mdi": "^1.1.55",
    "@iconify-json/ph": "^1.1.6",
    "@stylistic/eslint-plugin": "^1.7.2",
    "@types/node": "^18.16.0",
    "@unocss/eslint-plugin": "^0.59.4",
    "@unocss/preset-icons": "^0.53.4",
    "@unocss/transformer-attributify-jsx": "^0.53.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vueuse/components": "^10.5.0",
    "@vueuse/core": "^10.5.0",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^26.4.3",
    "electron-builder": "^24.13.3",
    "element-plus": "^2.6.3",
    "eslint": "^9.0.0",
    "eslint-plugin-format": "^0.1.1",
    "floating-vue": "2.0.0-beta.24",
    "mitt": "^3.0.1",
    "moment": "^2.29.4",
    "sass": "^1.53.0",
    "typescript": "^5.4.4",
    "unocss": "^0.53.4",
    "unplugin-vue-macros": "^2.9.0",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vue": "^3.4.21",
    "vue-i18n": "9.11.0",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.11",
    "vue3-perfect-scrollbar": "^1.6.1",
    "wait-on": "^6.0.1"
  },
  "packageManager": "pnpm@9.0.6"
}
