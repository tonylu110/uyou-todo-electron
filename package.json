{
  "name": "uyoutodo",
  "version": "3.2.0",
  "private": false,
  "author": {
    "name": "Anthony Lu",
    "email": "taruruchan0@gmail.com"
  },
  "license": "MIT",
  "main": "prebuild_electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron:servewin": "pnpm prebuild:dev && concurrently -k \"pnpm dev\" \"pnpm electron\"",
    "electron:servemac": "pnpm prebuild:dev && concurrently -k \"pnpm dev\" \"pnpm electron:mac\" ",
    "electron:servelinux": "pnpm prebuild:dev && concurrently -k \"pnpm dev\" \"pnpm electron:linux\"",
    "electron:buildwin": "pnpm prebuild:win && vite build && electron-builder",
    "electron:buildwinarm": "pnpm prebuild:win && vite build && electron-builder --win --arm64",
    "electron:buildmac": "pnpm prebuild:mac && vite build && electron-builder --config.npmRebuild=false -c.extraMetadata.main=prebuild_electron/mac/mainMac.js",
    "electron:buildlinux": "pnpm prebuild:linux && vite build && electron-builder --config.npmRebuild=false -c.extraMetadata.main=prebuild_electron/linux/mainLinux.js",
    "electron": "wait-on tcp:3000 && cross-env NODE_ENV=development electron .",
    "electron:mac": "wait-on tcp:3000 && cross-env NODE_ENV=development electron prebuild_electron/mac/mainMac.js",
    "electron:linux": "wait-on tcp:3000 && cross-env NODE_ENV=development electron prebuild_electron/linux/mainLinux.js --no-sandbox",
    "prebuild:win": "rolldown -c build_config/rolldown-win.config.js",
    "prebuild:mac": "rolldown -c build_config/rolldown-mac.config.js",
    "prebuild:linux": "rolldown -c build_config/rolldown-linux.config.js",
    "prebuild:dev": "rolldown -c",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "electron-is-dev": "^3.0.1",
    "electron-liquid-glass": "^1.0.1",
    "electron-store": "^8.1.0",
    "mica-electron": "^1.5.16"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@crazydos/vue-markdown": "^1.1.4",
    "@iconify-json/circle-flags": "^1.2.6",
    "@iconify-json/emojione-v1": "^1.2.1",
    "@iconify-json/f7": "^1.2.2",
    "@iconify-json/flagpack": "^1.2.3",
    "@iconify-json/fluent": "^1.2.28",
    "@iconify-json/icon-park-outline": "^1.2.2",
    "@iconify-json/logos": "^1.2.5",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/vscode-icons": "^1.2.24",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@tomjs/electron-devtools-installer": "^3.0.1",
    "@types/node": "^20.14.0",
    "@unocss/eslint-plugin": "^66.3.3",
    "@unocss/preset-icons": "^66.3.3",
    "@unocss/transformer-attributify-jsx": "^66.3.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue-vine/eslint-config": "^1.1.8",
    "@vueuse/components": "^13.6.0",
    "@vueuse/core": "^13.6.0",
    "concurrently": "^9.1.0",
    "cross-env": "^10.0.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "element-plus": "^2.11.0",
    "es-toolkit": "^1.39.8",
    "eslint": "^9.32.0",
    "eslint-plugin-format": "^1.0.1",
    "floating-vue": "5.2.2",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "ollama": "^0.5.17",
    "openai": "^5.12.2",
